<div id="body">


    <table>
        <thead>
            <tr>
                <th>Order_No</th>
                <th>Supplier</th>
                <th>Stock_Site</th>
                <th>Stock_Name</th>
                <th>Order_Date</th>
                <th>Last_Update</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: self.Products">
            <tr>
                <td data-bind="text: Order_No"></td>
                <td data-bind="text: Supplier"></td>
                <td data-bind="text: Stock_Site"></td>
                <td data-bind="text: Stock_Name"></td>
                <td data-bind="text: Order_Date"></td>
                <td data-bind="text: Last_Update"></td>

            </tr>
        </tbody>
        <tfoot>
        </tfoot>
    </table>
    <br />
    <div style="border-top: solid 2px #282828; width: 430px; height: 10px"> </div>




</div>